TestMyCode.run("testing hello_world function", function(assert){
    var result = helloworld();
   
    assert.equals("hello world!", result, "testing hello world function");
});

TestMyCode.run("testing DeepHouse function", function(assert){
    var result = DeepHouse();
   
    assert.equals("House head", result, "testing DeepHouse function");
});


TestMyCode.run("testing Programming function", function(assert){
    var result = Programming();
    
    assert.equals("I love Deep House", result, "testing Programming function");

});

TestMyCode.run("testing hello function", function(assert){
    var result = helloworld();
    
    assert.equals("hello world!", result, "testing hello function");

});


TestMyCode.run("testing hello uppercase function", function(assert){
    var result = upperCase("linkie");
    
    assert.equals("Hello World " + "linkie".toUpperCase(), result, "testing hello uppercase function");
});

TestMyCode.run("testing hello joe function", function(assert){
    var result = joe("bob");
    
    assert.equals("hello!", result, "testing hello joe function");

});
TestMyCode.run("testing sum numbers function", function(assert){
    var result = sum_numbers(6);
    
    assert.equals(21, result, "testing sum numbers function");

});

TestMyCode.run("testing number list function", function(assert){
    var result = number_list(7);
    
    assert.equals([1, 2, 3, 4, 5, 6, 7].toString(), result.toString(), "testing number list function");

});

